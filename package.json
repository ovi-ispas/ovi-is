{
  "name": "ovi-ispas.github.io",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "pnpm dlx http-server -o -p 3000 --proxy http://127.0.0.1:3000? ./build/client",
    "lint": "eslint --fix ./app",
    "format": "prettier --write .",
    "typecheck": "tsc",
    "clean": "rimraf ./node_modules ./build && pnpm install && pnpm build",
    "env:update": "gh variable set -f .env.production",
    "deps:update": "pnpm update --latest -i"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "dependencies": {
    "@jdion/tilt-react": "^1.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-three/drei": "^9.96.1",
    "@react-three/fiber": "^8.15.14",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.0",
    "framer-motion": "^10.18.0",
    "isbot": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-parallax-tilt": "^1.7.198",
    "react-particles": "^2.12.2",
    "react-slick": "^0.29.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.160.0",
    "tsparticles-engine": "^2.12.0",
    "tsparticles-slim": "^2.12.0"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@remix-run/dev": "^2.5.1",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rehype-pretty-code": "^0.12.5",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^4.0.0",
    "remix-development-tools": "^3.7.2",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "tm-themes": "^1.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
